model Note {
  id            Int      @id @default(autoincrement())
  url           String
  name          String
  original_name String
  size          Int
  extension     String
  created_at    DateTime @default(now()) @map("created_at")
  updated_at    DateTime @default(now()) @updatedAt @map("updated_at")

  @@map("notes")
}

